{
    "name": "dawen/elastic",
    "type": "library",
    "description": "Elasticsearch Component",
    "keywords": [],
    "homepage": "http://code.dawen.de/component",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Wendlandt",
            "email": "dan.wendlandt@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/event-dispatcher": "~2.1",
        "psr/log": "~1.0",
        "guzzlehttp/guzzle": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "squizlabs/php_codesniffer": "1.*",
        "phploc/phploc": "*",
        "pdepend/pdepend" : "1.1.0",
        "phpmd/phpmd" : "1.4.*",
        "sebastian/phpcpd": "*",
        "theseer/phpdox": "0.6.*",
        "mayflower/php-codebrowser": "~1.1"
    },
    "autoload": {
        "psr-4": { "Dawen\\Component\\Elastic\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "Dawen\\Component\\Elastic\\Tests\\": "tests/"
        }
    },
    "target-dir": "Dawen/Component/Elastic",
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev"
}